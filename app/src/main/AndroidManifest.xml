<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.card_capital">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CardCapital">
        <activity
            android:name=".actvities.AddAddressActivity"
            android:exported="false" />
        <activity
            android:name=".actvities.AddressActivity"
            android:exported="false" />
        <activity
            android:name=".actvities.PaymentActivity"
            android:exported="false" />
        <activity
            android:name=".actvities.CartActivity"
            android:exported="false" />
        <activity
            android:name=".actvities.SeeAllActivity"
            android:exported="false" />
        <activity
            android:name=".actvities.DetailedActivity"
            android:exported="false" />
        <activity
            android:name=".actvities.OnBoardScreenActivity"
            android:exported="false" />
        <activity
            android:name=".actvities.RegistrationActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".actvities.LoginActivity"
            android:exported="false" />
        <activity
            android:name=".actvities.MainActivity"
            android:exported="false" />
    </application>

</manifest>